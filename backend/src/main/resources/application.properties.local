spring.datasource.url=jdbc:postgresql://localhost:5432/tutorensystem-dhbw
spring.datasource.username=postgres
spring.datasource.password=postgres
spring.datasource.platform=postgres

spring.jpa.properties.hibernate.auto_quote_keyword=true
spring.jpa.show-sql=false
spring.jpa.hibernate.ddl-auto=create-drop

server.error.include-stacktrace=never

spring.thymeleaf.prefix=classpath:/mail-templates/

spring.devtools.restart.additional-exclude=logging/**
logging.config=./src/main/resources/logging/logback-spring.xml

// Add yourself to run the app
backend.app.jwtSecret=
backend.app.jwtExpirationMs=
backend.app.frontendUrl=
backend.app.minutesBetweenPasswordActions=

spring.mail.host=
spring.mail.port=
spring.mail.username=
spring.mail.password=
spring.mail.properties.mail.smtp.auth=
spring.mail.properties.mail.smtp.starttls.enable=
